{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noImplicitAny": false,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "app/_componants/MapComponent.jsx",
    "app/_componants/_dashboard/_mianpage/Hero_section_dash.jsx",
    "app/_componants/_dashboard/_mianpage/Blog_dash.jsx",
    "app/dashboard/FAQ/page.jsx",
    "app/dashboard/footerdash/page.jsx",
    "app/dashboard/adduser/page.jsx",
    "app/_componants/PagenationTable.jsx",
    "app/_componants/_dashboard/Topbar.jsx",
    "app/dashboard/testimonial/page.jsx"
  ],
  "exclude": ["node_modules"]
}
